@model IEnumerable<AjaxAndjQuery.Models.ViewModels.SpeakerViewModel>

@{
    ViewData["Title"] = "Speakers";
}

@section Scripts{
    <script src="~/js/CustomScripts/SpeakerScripts.js"></script>
}
<h2>@ViewData["Title"]</h2>
<div id="summary">
   <ul id="speakers">
    @foreach (var item in Model) 
    {  
          <li><a asp-action="details" asp-route-id="@item.Id" class="details">@item.FullName</a></li>
    }
  </ul>
 </div>
  <div id="speaker">
  </div>
  <br style="clear:both" />
  

