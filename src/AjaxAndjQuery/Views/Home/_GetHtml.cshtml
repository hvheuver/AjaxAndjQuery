@model IEnumerable<AjaxAndjQuery.Models.ViewModels.SpeakerViewModel>

<table class="table">
    <tr>
        <th>FullName</th>
        <th>Picture</th>
        <th>Bio</th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @item.FullName
        </td>
        <td>
            <img src="~/images/Speaker/@item.PictureUrl" 
                 title="@item.FullName" width="30" height="40" alt="Picture speaker"/>
        </td>
        <td>
            @item.Bio</td>
        <td></td>
    </tr>
}
</table>
